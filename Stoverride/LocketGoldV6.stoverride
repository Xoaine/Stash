#!name=Locket Gold 🏅 - NightmarketServer
#!desc=Mở khoá premium cho ứng dụng Locket Widget

name: Locket Gold 🏅 - NightmarketServer
desc: Mở khoá premium cho ứng dụng Locket Widget

http:
  mitm:
    - "api.revenuecat.com"
    - "raw.githubusercontent.com"

  script:
    - match: ^https:\/\/api\.revenuecat\.com\/.+\/(receipts$|subscribers\/[^/]+$)
      name: revenuecat
      type: response
      require-body: true
      timeout: 120

    - match: ^https:\/\/api\.revenuecat\.com\/.+\/(receipts|subscribers)
      name: deleteHeader
      type: request
      timeout: 120

script-providers:
  revenuecat:
    url: https://raw.githubusercontent.com/NightmarketServer/Locket/refs/heads/main/Locket_fix.js
    interval: 86400

  deleteHeader:
    url: https://raw.githubusercontent.com/NightmarketServer/Locket/refs/heads/main/deleteHeader.js
    interval: 86400